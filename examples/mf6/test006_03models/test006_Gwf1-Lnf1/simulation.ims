begin options
  PRINT_OPTION SUMMARY
end options

begin nonlinear
        outer_hclose 1.e-4
        outer_maximum 220
       #under_relaxation dbd
       #under_relaxation_theta 0.90
       #under_relaxation_kappa 0.07
       #under_relaxation_gamma 0.1
       #under_relaxation_momentum 0.00
       #BACKTRACKING_NUMBER 20
       #BACKTRACKING_TOLERANCE 1.5e25
       #BACKTRACKING_REDUCTION_FACTOR 0.2
       #BACKTRACKING_RESIDUAL_LIMIT 10.0
        linear_solver xmd
end nonlinear

begin xmd
        inner_hclose 1.0e-5
        inner_maximum 600
        LINEAR_ACCELERATION bicgstab
        reordering_method none
        PRECONDITIONER_LEVELS 0
        NUMBER_ORTHOGONALIZATIONS 12
        inner_rclose 0.
        #PRECONDITIONER_DROP_TOLERANCE 1.0e-5
end xmd
