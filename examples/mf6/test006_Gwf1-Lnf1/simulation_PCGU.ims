begin options
  PRINT_OPTION SUMMARY
end options

begin nonlinear
	outer_hclose 1.e-8
	outer_maximum 1000 
	under_relaxation none
	linear_solver pcgu
end nonlinear

begin pcgu
	inner_hclose 1.0e-8
	inner_rclose 0.01
	inner_maximum 1000
	linear_acceleration cg
	preconditioner milu0
	scaling_method none
	REORDERING_METHOD none
	relaxation_factor 0.97
end pcgu

