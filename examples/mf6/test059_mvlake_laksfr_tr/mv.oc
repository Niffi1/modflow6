BEGIN OPTIONS
  HEAD FILEOUT mv.hds
  BUDGET FILEOUT mv.cbc
  HEAD PRINT_FORMAT COLUMNS 25 WIDTH 7 DIGITS 2 FIXED
END OPTIONS

BEGIN PERIOD 1
  SAVE BUDGET ALL
  PRINT BUDGET ALL
  SAVE HEAD ALL
END PERIOD

BEGIN PERIOD 2
  SAVE BUDGET FREQUENCY 10
  PRINT BUDGET FREQUENCY 10
  SAVE HEAD FREQUENCY 10
END PERIOD

BEGIN PERIOD 3
  SAVE BUDGET LAST
  PRINT BUDGET LAST
  SAVE HEAD LAST
END PERIOD

